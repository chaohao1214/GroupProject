<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/OCTranspo"
        android:layout_width="411dp"
        android:layout_height="wrap_content"
        android:text="OCTranspo Bus"
        app:layout_constraintBottom_toTopOf="@+id/CarCharge"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/CarCharge"
        android:layout_width="411dp"
        android:layout_height="wrap_content"

        android:layout_marginEnd="92dp"
        android:layout_marginRight="92dp"
        android:layout_marginBottom="100dp"
        android:text="Electric Car Charging"
        app:layout_constraintBottom_toTopOf="@+id/Soccer"
        app:layout_constraintEnd_toEndOf="parent" />


    <ImageButton
        android:id="@+id/movie_info"
        android:layout_width="282dp"
        android:layout_height="110dp"
        android:layout_marginStart="64dp"
        android:layout_marginLeft="64dp"
        android:layout_marginEnd="64dp"
        android:layout_marginRight="64dp"
        android:layout_marginBottom="88dp"
        android:contentDescription="movieFinder"
        android:scaleType="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/movie_icon1"
        tools:ignore="MissingConstraints" />

        android:text="Electric Car Charging"
        app:layout_constraintBottom_toTopOf="@+id/MovieInfo"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="16dp" />

    <Button
        android:id="@+id/Soccer"
        android:layout_width="406dp"
        android:layout_height="48dp"
        android:text="Soccer Game"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/movie_info"
        tools:layout_editor_absoluteX="5dp" />



</androidx.constraintlayout.widget.ConstraintLayout>